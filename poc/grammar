This file is for documentation purposes only.

FLOAT: (-|+)?[0-9]+\.[0-9]*
INT: (-|+)?[0-9]+

literal matches: (),:[]

NAME: [^(),:[\]]+

expr = number | tuple | pnvar | func

tuple = ( expr ) | ( expr tuple-rest )
tuple-rest = , expr | , expr tuple-rest

number = INT | FLOAT
pnvar = : NAME

func = fx NAME func-body
func-body = [ tuple expr func-body | [ tuple expr
